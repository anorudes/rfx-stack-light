  "scripts": {
    "lint": "eslint src --ext .jsx,.js",
    "clean:client": "rm -rf ./public/build",
    "clean:server": "rm -rf ./server.bundle.js",
    "clean:modules": "rm -rf ./node_modules && npm cache clean",
    "build:all": "npm run build:client && npm run build:server",
    "build:client": "npm run clean:client && NODE_ENV=production webpack -p",
    "build:server": "npm run clean:server && NODE_ENV=production webpack -p",
    "server:dev": "NODE_ENV=development node ./server.js",
    "server:prod": "NODE_ENV=production node ./server.bundle.js"
  },
